@model List<ApplicationCore.Models.MovieCardModel>
@{
    ViewData["Title"] = "Home Page";
}

  <div class="rounded">
      <div class="container-fulid">
          <div class="row">
              <!-- without prtial view -->
            @foreach(var movie in Model)
            {
              <div class="col-6 col-xl-2 col-sm-2 col-lg-3  col-md-4">
                  <partial name="_MovieCard" model="movie">
                  </partial>
              </div>
            }
          </div>
      </div>
  </div>

